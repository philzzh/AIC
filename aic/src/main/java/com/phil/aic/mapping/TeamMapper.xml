<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.phil.aic.dao.TeamMapper" >
  <resultMap id="BaseResultMap" type="com.phil.aic.bean.TeamKey" >
    <id column="team_id" property="teamId" jdbcType="VARCHAR" />
    <id column="member" property="member" jdbcType="VARCHAR" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="com.phil.aic.bean.TeamKey" >
    delete from team
    where team_id = #{teamId,jdbcType=VARCHAR}
      and member = #{member,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="com.phil.aic.bean.TeamKey" >
    insert into team (team_id, member)
    values (#{teamId,jdbcType=VARCHAR}, #{member,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.phil.aic.bean.TeamKey" >
    insert into team
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="teamId != null" >
        team_id,
      </if>
      <if test="member != null" >
        member,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="teamId != null" >
        #{teamId,jdbcType=VARCHAR},
      </if>
      <if test="member != null" >
        #{member,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>